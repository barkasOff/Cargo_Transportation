<UserControl
    x:Class="Cargo_Transportation.Pages.PageHost"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:Cargo_Transportation.Pages"
    xmlns:attachedProperties="clr-namespace:Cargo_Transportation.AttachedProperties"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">

    <Grid>

        <Frame x:Name="OldPage" attachedProperties:NoFrameHistory.Value="True" />

        <Frame x:Name="NewPage" attachedProperties:NoFrameHistory.Value="True" />

    </Grid>

</UserControl>
